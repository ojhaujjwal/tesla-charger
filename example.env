TESLA_OAUTH2_CLIENT_ID=xxxxx
TESLA_OAUTH2_CLIENT_SECRET=xxxxx


## Can be generated using "npm run cmd:generate-refresh-token"
TESLA_OAUTH2_REFRESH_TOKEN=xxxx

TESLA_VIN=LRWYHXXXXXXXXX
TESLA_APP_DOMAIN=example.com
TESLA_TOKEN_FILE=.access-token

# Name of the private key used for authentication with Tesla API. 
