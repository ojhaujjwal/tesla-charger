# Path to the private key file. 
# This is optional if the key is stored in the default location (e.g., ~/.tesla-keys/). 
# Not required if TESLA_KEY_NAME is provided.
TESLA_KEY_FILE=/Users/xxxxx/tesla-private-key.pem


INFLUX_URL=http://localhost:8086
INFLUX_TOKEN=xxxxxxx
INFLUX_ORG=xxxxxx
INFLUX_BUCKET=xxxxxx


SENTRY_DSN=https://xxxxxxxx

